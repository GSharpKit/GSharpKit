{
    "upgrade_guid": "67246d64-b0ee-11e8-af4d-708bcda85715",
    "version": "@VERSION@.@VERSION_BUILD@",
    "product_name": "GSharpKit",
    "manufacturer": "XMedicus Systems ApS",
    "name": "GSharpKit",
    "name_base": "GSharpKit",
    "comments": "GSharpKit Framework",
    "installdir": "GSharpKit",
    "license_file": "License.rtf",
    "arch": @ARCH_NO@,
    "need_msvcrt": false,
    "major_upgrade": {
        "AllowDowngrades": "yes",
        "IgnoreRemoveFailure": "yes"
    },
    "registry_entries": [
    	{
    		"root": "HKLM",
    		"key": "SOFTWARE\\GSharpKit",
    		"action": "createAndRemoveOnUninstall",
    		"name": "Install_Dir",
    		"type": "string",
    		"value": "[INSTALLDIR]",
    		"key_path": "no"
    	},
        {
                "root": "HKLM",
                "key": "SOFTWARE\\GSharpKit",
                "action": "createAndRemoveOnUninstall",
                "name": "Version",
                "type": "string",
                "value": "@VERSION@",
                "key_path": "no"
        }
    ],
    "custom_actions": [
        {
                "id": "CertAction",
                "exe_command": "/C \"[INSTALLDIR]bin\\mono.exe\" \"[INSTALLDIR]bin\\cert-sync.exe\" --quiet \"[INSTALLDIR]bin\\ca-bundle.crt\"",
                "execute": "immediate",
                "return": "ignore",
                "impersonate": "no",
                "after": "InstallFiles"
        },
        {
                "id": "VCRedist",
                "exe_command": "/C \"[INSTALLDIR]bin\\VC_redist.@ARCH_SHORT@.exe\" /install /quiet /norestart",
                "execute": "immediate",
                "return": "ignore",
                "impersonate": "no",
                "after": "InstallFiles"
        }
    ],
    "parts": [
        {
         "id": "GSharpKit",
         "title": "GSharpKit",
         "description": "GSharpKit Framework",
         "absent": "disallow",
         "staged_dir": "framework"
        }
    ]
}
