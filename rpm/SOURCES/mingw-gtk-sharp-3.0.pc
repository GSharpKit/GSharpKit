prefix=@PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/bin
gapidir=${prefix}/share/gapi-3.0


Name: Gtk#
Description: Gtk# - Gtk .NET Binding
Version: @VERSION@
Cflags: -I:${gapidir}/CairoSharp-api.xml -I:${gapidir}/PangoSharp-api.xml -I:${gapidir}/AtkSharp-api.xml -I:${gapidir}/GtkSharp-api.xml
Libs: -r:Facades/netstandard.dll -r:${libdir}/CairoSharp.dll -r:${libdir}/PangoSharp.dll -r:${libdir}/AtkSharp.dll -r:${libdir}/GtkSharp.dll
Requires: glib-sharp-3.0 gio-sharp-3.0 gdk-sharp-3.0
